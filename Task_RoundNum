using System;

namespace Hello
{
    class Program
    {
        static void Main(string[] args)
        {   
            int a = 12;
            int b = 8;
            double c = (double)a/b;
            Console.WriteLine("c " + c);
            int result = 0;
            result = (int)Math.Round(c);
            Console.WriteLine("result " + result);
                 
        } 
    }
} 
